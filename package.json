{
  "name": "scam-checker",
  "version": "0.1.0",
  "private": true,
  "description": "A comprehensive URL analysis tool that helps users identify potentially malicious websites through multi-factor risk assessment.",
  "scripts": {
    "dev": "next dev",
    "build": "npm run lint && npm run type-check && next build",
    "build:only": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "check": "npm run lint && npm run type-check",
    "test": "jest",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "playwright test",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e:services": "jest --config tests/config/jest.e2e.services.config.js --runInBand",
    "test:e2e:services:watch": "jest --config tests/config/jest.e2e.services.config.js --watch",
    "test:e2e:services:reputation": "jest --config tests/config/jest.e2e.services.config.js --testNamePattern='ReputationService'",
    "test:e2e:services:whois": "jest --config tests/config/jest.e2e.services.config.js --testNamePattern='WhoisService'",
    "test:e2e:services:ai": "jest --config tests/config/jest.e2e.services.config.js --testNamePattern='AIService'",
    "test:e2e:services:ssl": "jest --config tests/config/jest.e2e.services.config.js --testNamePattern='SSLService'",
    "test:e2e:services:ci": "jest --config tests/config/jest.e2e.services.config.js --ci --silent"
  },
  "dependencies": {
    "autoprefixer": "^10.4.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.400.0",
    "next": "^15.0.0",
    "openai": "^4.0.0",
    "pino": "^9.9.0",
    "pino-lambda": "^4.4.1",
    "postcss": "^8.4.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.0.0",
    "whois": "^2.15.0",
    "zod": "^3.22.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.44.0",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^14.3.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/supertest": "^6.0.3",
    "dotenv": "^17.2.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "pino-pretty": "^13.1.1",
    "prettier": "^3.3.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "keywords": [
    "security",
    "url-analysis",
    "scam-detection",
    "nextjs",
    "typescript",
    "aws-lambda"
  ],
  "author": "Development Team",
  "license": "MIT"
}
