{
  "name": "scam-checker",
  "version": "0.1.0",
  "private": true,
  "description": "A comprehensive URL analysis tool that helps users identify potentially malicious websites through multi-factor risk assessment.",
  "scripts": {
    "dev": "next dev",
    "build": "npm run lint && npm run type-check && next build",
    "build:only": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "check": "npm run lint && npm run type-check",
    "test": "jest",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "playwright test",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e:services": "jest --config tests/config/jest.e2e.services.config.js --runInBand",
    "test:e2e:services:watch": "jest --config tests/config/jest.e2e.services.config.js --watch",
    "test:e2e:services:reputation": "jest --config tests/config/jest.e2e.services.config.js --testNamePattern='ReputationService'",
    "test:e2e:services:whois": "jest --config tests/config/jest.e2e.services.config.js --testNamePattern='WhoisService'",
    "test:e2e:services:ai": "jest --config tests/config/jest.e2e.services.config.js --testNamePattern='AIService'",
    "test:e2e:services:ssl": "jest --config tests/config/jest.e2e.services.config.js --testNamePattern='SSLService'",
    "test:e2e:services:ci": "jest --config tests/config/jest.e2e.services.config.js --ci --silent",
    "ui:list": "npx shadcn@latest list",
    "ui:add": "npx shadcn@latest add",
    "ui:check": "echo 'Check https://ui.shadcn.com/docs/components for available components'",
    "ui:blocks": "echo 'Browse https://ui.shadcn.com/blocks for UI patterns'"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "autoprefixer": "^10.4.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^11.18.2",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.2",
    "lucide-react": "^0.542.0",
    "next": "^15.5.2",
    "next-themes": "^0.4.6",
    "openai": "^5.16.0",
    "pino": "^9.9.0",
    "pino-lambda": "^4.4.1",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.2",
    "whois": "^2.15.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@next/eslint-plugin-next": "^15.5.2",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/parser": "^8.41.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.34.0",
    "eslint-config-next": "^15.5.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^30.1.1",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.1.1",
    "jest-fetch-mock": "^3.0.3",
    "pino-pretty": "^13.1.1",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "keywords": [
    "security",
    "url-analysis",
    "scam-detection",
    "nextjs",
    "typescript",
    "aws-lambda"
  ],
  "author": "Development Team",
  "license": "MIT"
}
