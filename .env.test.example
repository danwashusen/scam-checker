# E2E Service Testing Configuration
# Copy to .env.test and add your test API keys

# Google Safe Browsing (required for reputation tests)
GOOGLE_SAFE_BROWSING_TEST_API_KEY=

# OpenAI (required for AI tests)
# Use a test account with limited quota
OPENAI_TEST_API_KEY=

# Test Configuration
E2E_TEST_TIMEOUT=60000
E2E_TEST_RETRY_DELAY=2000
E2E_TEST_MAX_RETRIES=1

# CI Configuration
CI=false